generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id         Int     @id @default(autoincrement())
    email      String  @unique
    name       String  @db.VarChar(100)
    password   String  @db.VarChar(255)
    profilePic String?
}
